{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "test",
      "camelcase": "Test",
      "title": "Test",
      "scope": "source.test",
      "file-types": [
        "test"
      ],
      "injection-regex": "^test$",
      "class-name": "TreeSitterTest"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Test grammar for tree-sitter",
    "authors": [
      {
        "name": "daniel",
        "email": "ernandes"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-test"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}

